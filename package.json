{
  "name": "gift-frame",
  "main": "index.js",
  "version": "1.0.0",
  "scheme": "gift-frame",
  "scripts": {
    "dev": "expo start -c --ios",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "storybook-generate": "sb-rn-get-stories",
    "supabase:gen": "npx supabase gen types --lang=typescript --project-id ejltwptnbyvhxfkadery --schema public > ./types/database.types.ts",
    "export:web": "npx expo export -p web",
    "check:type": "npx tsc --noEmit",
    "install:fold": "npx expo run:android --device SM_F946N",
    "install:fold:relese": "npx expo run:android --device SM_F946N --variant release",
    "build:production": "npx eas-cli build --platform android --profile production",
    "build:deploy": "npx eas-cli build --platform android --profile production --auto-submit",
    "submit:android": "npx eas-cli submit --platform android --profile production"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.0",
    "@hookform/resolvers": "^3.9.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.0.2",
    "@rn-primitives/portal": "~1.0.3",
    "@rn-primitives/slot": "~1.0.3",
    "@rn-primitives/types": "~1.0.3",
    "@supabase/supabase-js": "^2.45.6",
    "@tanstack/react-query": "^5.59.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.13",
    "expo": "^52.0.7",
    "expo-asset": "~11.0.1",
    "expo-dev-client": "~5.0.1",
    "expo-font": "~13.0.1",
    "expo-image": "~2.0.0",
    "expo-linking": "~7.0.2",
    "expo-navigation-bar": "~4.0.3",
    "expo-router": "~4.0.15",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "nativewind": "^4.0.33",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.53.1",
    "react-native": "0.76.3",
    "react-native-safe-area-context": "4.12.0",
    "react-native-svg": "15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "compression": "^1.7.5",
    "express": "^4.21.1",
    "metro": "^0.81.0",
    "morgan": "^1.10.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "typescript": "~5.3.3"
  },
  "private": true
}
